{
    "manifest_version": 2,
    "name": "Video Downloader",
    "version": "0.1.0",
    "description": "Video Video Downloader From Youtube And Facebook",
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["./sweetalert.min.js","./main.js"],
            "type": "module",
            "css": ["./style.css"]
        }
    ],
    "browser_action": {
        "default_popup": "./index.html"
    },
    "icons":{
        "128": "./icons/128.png",
        "48": "./icons/48.png",
        "32": "./icons/32.png",
        "16": "./icons/16.png"
    }
}